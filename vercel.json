{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/app/(.*)", "destination": "/" },
    { "source": "/admin/(.*)", "destination": "/" },
    { "source": "/login", "destination": "/" },
    { "source": "/register", "destination": "/" }
  ]
}